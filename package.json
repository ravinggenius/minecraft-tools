{
  "name": "minecraft-tools",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "npm@11.3.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "migrate": "src/scripts/next-env atlas schema apply --env default",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint . --ignore-pattern '.next/*' --ignore-pattern 'node_modules/*'",
    "lint:fix": "npm run lint:check -- --fix",
    "load": "npm run load:releases && npm run load:items",
    "load:items": "src/scripts/next-env bun ./src/scripts/import-items.ts",
    "load:releases": "src/scripts/next-env bun ./src/scripts/import-releases.ts",
    "test:integration": "NODE_ENV=test playwright test",
    "test:unit": "NODE_ENV=test src/scripts/next-env vitest"
  },
  "dependencies": {
    "@hapi/iron": "^7.0.1",
    "@roarr/cli": "^5.12.4",
    "@tanstack/react-form": "^1.28.0",
    "camelcase": "^9.0.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "dedent": "^1.7.1",
    "i18next": "^25.8.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-resources-to-backend": "^1.2.1",
    "intl-parse-accept-language": "^1.0.0",
    "next": "^16.1.6",
    "nodemailer": "^8.0.1",
    "oslo": "^1.2.1",
    "preferred-locale": "^2.0.2",
    "rambda": "^11.1.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "reformdata": "^0.2.2",
    "roarr": "^7.21.4",
    "search-query-parser": "^1.6.0",
    "server-only": "^0.0.1",
    "slonik": "^48.10.0",
    "slonik-interceptor-query-logging": "^48.10.0",
    "slonik-interceptor-query-normalisation": "^1.1.10",
    "typescript": "^5.9.3",
    "typescript-cookie": "^1.0.6",
    "validator": "^13.15.26",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@c-ehrlich/eslint-plugin-use-server": "^0.0.6",
    "@next/env": "^16.1.6",
    "@playwright/test": "^1.58.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.2",
    "@total-typescript/ts-reset": "^0.6.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.2",
    "@types/node": "^25.2.3",
    "@types/nodemailer": "^7.0.9",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@types/validator": "^13.15.10",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-sql": "^3.2.1",
    "jsdom": "^28.0.0",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "vite-tsconfig-paths": "^6.1.0",
    "vitest": "^4.0.18"
  }
}
